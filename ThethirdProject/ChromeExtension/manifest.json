{
  "manifest_version": 3,
  "name": "QuickSave Links",
  "version": "1.0",
  "description": "Effortlessly save and manage your favorite links.",
  "permissions": ["tabs", "storage"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  }
}
